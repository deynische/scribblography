<ul class="social-icons">
<% for (var i in theme.social_links) { %>
    <% if (i == 'facebook') { %>
        <li><a href="<%- url_for(theme.social_links[i]) %>" class="fab fa-facebook" title="Facebook"></a></li>
    <% } %>
    <% if (i == 'instagram') { %>
        <li><a href="<%- url_for(theme.social_links[i]) %>" class="fab fa-instagram" title="Instagram"></a></li>
    <% } %>
    <% if (i == 'twitter') { %>
        <li><a href="<%- url_for(theme.social_links[i]) %>" class="fab fa-twitter" title="Twitter"></a></li>
    <% } %>
    <% if (i == 'pinterest') { %>
        <li><a href="<%- url_for(theme.social_links[i]) %>" class="fab fa-pinterest" title="Pinterest"></a></li>
    <% } %>
    <% if (i == 'goodreads') { %>
