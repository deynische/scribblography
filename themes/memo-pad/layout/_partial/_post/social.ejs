    <% } %>
    <% if (i == 'behance') { %>
        <li><a href="<%- url_for(theme.social_links[i]) %>" class="fab fa-behance" title="Behance"></a></li>
    <% } %>
    <% if (i == 'dribbble') { %>
        <li><a href="<%- url_for(theme.social_links[i]) %>" class="fab fa-dribbble" title="Dribbble"></a></li>
